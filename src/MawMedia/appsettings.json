{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Https": {
                "Url": "https://*:8090"
            }
        },
        "Certificates": {
            "Default": {
                "Path": "/media-secrets/maw-media.pfx"
            }
        }
    },
    "AllowedHosts": "*",
    "DataProtection": {
        "Path": "/media-data-protection"
    },
    "Npgsql": {
        "ConnectionString": "Host=localhost;Port=5432;Database=maw_media;Username=svc_maw_media;Passfile=/media-secrets/npgsql-passfile-svc_maw_media;Application Name=maw-media;Max Auto Prepare=100"
    },
    "ForwardedHeaders": {
        "KnownNetworks": [
            "192.168.0.0/16",
            "169.254.1.2/32"
        ]
    },
    "CorsOriginUrls": [
        "https://photos.mikeandwan.us"
    ],
    "Assets": {
        "RootDirectory": "/media-assets"
    },
    "CategoryDownload": {
        "RootDirectory": "/media-download",
        "CleanIntervalInMinutes": 15,
        "MinAgeBeforeDeleteInMinutes": 300
    },
    "Upload": {
        "RootDirectory": "/media-upload"
    },
    "OAuth": {
        "Authority": "https://login.mikeandwan.us/",
        "Audience": "https://media.mikeandwan.us"
    }
}
